package Cocos2dx {
    class CCCore {
        enum CC_OBJECT_TYPE{
            CC_OBJ_APPLICATION,
            CC_OBJ_SCENE,
            CC_OBJ_DIRECTOR
        };
        long hcore;
        CCCore(){
            
        }
        CCCore(long h){
            hcore = h;
        }
        
        bool create(CC_OBJECT_TYPE type){
            hcore = CCNative.create_xxobject(CCNative.NATIVE_SYSCALL_ID.CC_CREATEOBJECT, this, type);
            return hcore != 0;
        }
    };
};