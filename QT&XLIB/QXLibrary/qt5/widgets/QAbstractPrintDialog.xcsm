package Qt{
    public class QAbstractPrintDialog : QDialog{

        public enum PrintDialogOption {
            None                    = 0x0000, // obsolete
            PrintToFile             = 0x0001,
            PrintSelection          = 0x0002,
            PrintPageRange          = 0x0004,
            PrintShowPageSize       = 0x0008,
            PrintCollateCopies      = 0x0010,
            DontUseSheet            = 0x0020,
            PrintCurrentPage        = 0x0040
        };
        
        public enum PrintRange {
            AllPages,
            Selection,
            PageRange,
            CurrentPage
        };
        
        public QAbstractPrintDialog(){
        
        }
        
